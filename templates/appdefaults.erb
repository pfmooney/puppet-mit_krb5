
<%- fields = [
    'debug',
    'ticket_lifetime',
    'renew_lifetime',
    'forwardable',
    'krb4_convert',
    'ignore_afs',
]
#
array_fields = ['kdc', 'admin_server']
output = []
scope_hash = scope.to_hash
fields.each do |k|
  if scope_hash.include?(k)
    value = scope_hash[k]
    output.push("#{k} = #{value}")
  end
end
-%>
    <%= @title %> = {
        <%= output.join("\n        ") %>
    }

